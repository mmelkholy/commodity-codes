  "included": [{
      "id": "1",
      "type": "section",
      "attributes": {
        "numeral": "I",
        "title": "Live animals; animal products",
        "position": 1,
        "section_note": "1. Any reference in this section to a particular genus or species of an animal, except where the context otherwise requires, includes a reference to the young of that genus or species.\r\n2. Except where the context otherwise requires, throughout the nomenclature any reference to 'dried' products also covers products which have been dehydrated, evaporated or freeze-dried.\r\n"
      }
    },
